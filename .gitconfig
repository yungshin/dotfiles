[user]
	name = yungshin
	email = yungshin.chien@gmail.com
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
  unstage = reset HEAD
  hist = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	type = cat-file -t
	dump = cat-file -p
[color]
	ui = true
[core]
	editor = vim
	autocrlf = input
